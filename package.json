    "predeploy": "yarn build && cd example/ && yarn build && cd ../",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "croods": "1.x",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
